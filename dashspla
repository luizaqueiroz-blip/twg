    <div id="modal" class="fixed inset-0 bg-black bg-opacity-70 hidden items-center justify-center p-4 z-50">
        <div class="bg-white rounded-xl shadow-2xl w-full max-w-2xl transform transition-all modal-content overflow-y-auto">
            <div class="sticky top-0 bg-white p-6 border-b border-slate-200 flex justify-between items-center z-10">
                <h2 id="modal-title" class="text-xl font-bold text-slate-900"></h2>
                <button id="modal-close" class="text-slate-500 hover:text-slate-800 text-3xl leading-none">&times;</button>
            </div>
            <div id="modal-body" class="p-6 space-y-6">
            </div>
        </div>
    </div>


    <script>
        document.addEventListener('DOMContentLoaded', () => {
            let allContentData = [];
            let allExecutiveSummaries = {};
            let chartInstance = null;
            
            const pipelineContainer = document.querySelector('#content-pipeline');
            const modal = document.getElementById('modal');
            const modalCloseBtn = document.getElementById('modal-close');
            const monthSelector = document.getElementById('month-selector');
            const filterType = document.getElementById('filter-type');
            const filterFormat = document.getElementById('filter-format');
            const filterStatus = document.getElementById('filter-status');

            function populateFilters(contentData) {
                const formats = [...new Set(contentData.map(item => item.format))].filter(Boolean);
                filterFormat.innerHTML = '<option value="all">Todos los Formatos</option>';
                formats.sort().forEach(format => {
                    const option = document.createElement('option');
                    option.value = format;
                    option.textContent = format;
                    filterFormat.appendChild(option);
                });
            }

            function getStatusBadge(status) {
                return status === 'Confirmado' ? `<span class="inline-block bg-green-100 text-green-800 text-xs font-semibold px-2 py-0.5 rounded-full">Confirmado</span>` : `<span class="inline-block bg-yellow-100 text-yellow-800 text-xs font-semibold px-2 py-0.5 rounded-full">Tentativo</span>`;
            }

            function getTypeBadge(type) {
                return type === 'Original' ? `<span class="inline-block bg-green-100 text-green-800 text-xs font-semibold px-2 py-0.5 rounded-full">Original</span>` : `<span class="inline-block bg-slate-100 text-slate-800 text-xs font-semibold px-2 py-0.5 rounded-full">Traducci√≥n</span>`;
            }
            
            function formatDate(dateObj) {
                if (!dateObj || !(dateObj instanceof Date)) return 'Sin fecha';
                return dateObj.toLocaleDateString('es-ES', { day: '2-digit', month: 'short' }).replace('.', '');
            }

            function renderPipeline(data) {
                pipelineContainer.innerHTML = '';
                if (data.length === 0) {
                    pipelineContainer.innerHTML = `<p class="md:col-span-2 text-center text-slate-500 py-10">Ning√∫n contenido coincide con los filtros actuales.</p>`;
                    return;
                }
                data.sort((a, b) => a.publishDate - b.publishDate).forEach((item) => {
                    const isTBP = item.synopsis === 'TBP.';
                    
                    const cardLink = document.createElement('a');
                    cardLink.href = isTBP ? '#' : item.link;
                    if(!isTBP) cardLink.target = '_blank';
                    cardLink.rel = 'noopener noreferrer';
                    cardLink.className = `bg-white rounded-xl shadow-sm border border-slate-200 p-5 flex flex-col card-hover-effect ${isTBP ? 'opacity-70 cursor-default' : ''}`;

                    cardLink.innerHTML = `
                        <div class="flex-grow">
                            <div class="flex justify-between items-start mb-3">
                                <p class="text-sm text-slate-500 font-bold bg-slate-100 px-2 py-1 rounded-md">${formatDate(item.publishDate)}</p>
                                ${getStatusBadge(item.status)}
                            </div>
                            <h3 class="text-base font-bold text-slate-900 mb-2">${item.title}</h3>
                            <p class="text-sm text-slate-600 mb-4 line-clamp-2">${isTBP ? 'Detalles pr√≥ximamente. Este contenido est√° en fase de planificaci√≥n.' : item.synopsis}</p>
                        </div>
                        <div class="mt-auto pt-4 border-t border-slate-100 flex justify-between items-center">
                             <div class="flex items-center gap-2">
                                ${getTypeBadge(item.type)}
                                <span class="text-xs font-medium text-slate-500">${item.format}</span>
                            </div>
                            <button class="ver-detalhes text-sm font-bold text-green-600 hover:underline">Ver detalles +</button>
                        </div>
                    `;
                    
                    const detailsButton = cardLink.querySelector('.ver-detalhes');
                    detailsButton.addEventListener('click', (e) => {
                        e.preventDefault();
                        e.stopPropagation();
                        openModal(item);
                    });

                    if(isTBP) {
                         cardLink.addEventListener('click', (e) => e.preventDefault());
                    }

                    pipelineContainer.appendChild(cardLink);
                });
            }

            function openModal(item) {
                document.getElementById('modal-title').textContent = item.title;
                const modalBody = document.getElementById('modal-body');
                const modalContent = modal.querySelector('.modal-content');

                const displayOrTBP = (text) => text === 'TBP' || text === 'TBP.' || text === 'TBC' ? '<span class="text-slate-500 italic">A definir.</span>' : text;
                const isTBP = item.starter === 'TBP' || item.starter === 'TBP.' || item.starter === 'TBC';

                modalBody.innerHTML = `
                    <div><h4 class="text-sm font-bold text-slate-500 uppercase tracking-wider mb-2">Sinopsis</h4><p class="text-slate-700">${displayOrTBP(item.synopsis)}</p></div>
                    <div class="p-5 bg-slate-100 rounded-lg">
                        <div class="flex justify-between items-center mb-2">
                            <h4 class="text-sm font-bold text-slate-600 uppercase tracking-wider">Texto para LinkedIn</h4>
                            <button id="copy-btn" class="px-3 py-1 bg-green-500 text-white text-xs font-semibold rounded-md hover:bg-green-600 transition-colors ${isTBP ? 'hidden' : ''}">Copiar</button>
                        </div>
                        <p id="starter-text" class="text-slate-700">"${displayOrTBP(item.starter)}"</p>
                    </div>`;
                
                modal.classList.remove('hidden');
                modal.classList.add('flex');
                document.body.classList.add('modal-open');
                modalContent.scrollTop = 0;
                
                if (!isTBP) {
                    document.getElementById('copy-btn').addEventListener('click', (e) => {
                        const textToCopy = document.getElementById('starter-text').textContent;
                        const textArea = document.createElement('textarea');
                        textArea.value = textToCopy;
                        document.body.appendChild(textArea);
                        textArea.select();
                        try {
                            document.execCommand('copy');
                            e.target.textContent = '¬°Copiado!';
                            setTimeout(() => { e.target.textContent = 'Copiar'; }, 2000);
                        } catch (err) { console.error('Error al copiar texto: ', err); }
                        document.body.removeChild(textArea);
                    });
                }
            }

            function closeModal() {
                modal.classList.add('hidden');
                modal.classList.remove('flex');
                document.body.classList.remove('modal-open');
            }

            function applyFilters() {
                const selectedMonth = monthSelector.value;
                let filteredData = allContentData.filter(item => {
                    if (!item.publishDate) return false;
                    const itemMonthKey = `${item.publishDate.getUTCFullYear()}-${item.publishDate.getUTCMonth()}`;
                    return itemMonthKey === selectedMonth;
                });

                if (filterType.value !== 'all') filteredData = filteredData.filter(item => item.type === filterType.value);
                if (filterFormat.value !== 'all') filteredData = filteredData.filter(item => item.format === filterFormat.value);
                if (filterStatus.value !== 'all') filteredData = filteredData.filter(item => item.status === filterStatus.value);
                
                renderPipeline(filteredData);
            }

            function initChart(contentData) {
                const ctx = document.getElementById('contentMixChart').getContext('2d');
                if (chartInstance) chartInstance.destroy();
                
                const typeCounts = contentData.reduce((acc, item) => {
                    if (item.type) acc[item.type] = (acc[item.type] || 0) + 1;
                    return acc;
                }, {});

                chartInstance = new Chart(ctx, {
                    type: 'doughnut',
                    data: {
                        labels: Object.keys(typeCounts),
                        datasets: [{ data: Object.values(typeCounts), backgroundColor: ['#16a34a', '#e2e8f0'], borderColor: ['#f8fafc', '#f8fafc'], borderWidth: 4 }]
                    },
                    options: {
                        responsive: true,
                        maintainAspectRatio: false,
                        plugins: {
                            legend: { position: 'bottom' },
                            tooltip: { callbacks: { label: (context) => `${context.label || ''}: ${context.parsed || ''}` } }
                        },
                        cutout: '60%'
                    }
                });
            }

            function renderForMonth(monthKey) {
                const monthData = allContentData.filter(item => {
                    if (!item.publishDate) return false;
                    const itemMonthKey = `${item.publishDate.getUTCFullYear()}-${item.publishDate.getUTCMonth()}`;
                    return itemMonthKey === monthKey;
                });

                if (monthData.length === 0) {
                    pipelineContainer.innerHTML = `<p class="md:col-span-2 text-center text-slate-500 py-10">No hay contenido disponible para este mes.</p>`;
                    document.getElementById('strategic-theme').textContent = 'N/A';
                    document.getElementById('business-goal').textContent = 'N/A';
                    const date = new Date(Date.UTC(monthKey.split('-')[0], monthKey.split('-')[1]));
                    document.getElementById('month-display').textContent = `Para ${date.toLocaleString('es-ES', { month: 'long', year: 'numeric', timeZone: 'UTC' })}`;

                } else {
                    const summary = allExecutiveSummaries[monthKey] || { theme: 'N/A', goal: 'N/A' };
                    const monthDate = monthData[0].publishDate;
                    document.getElementById('month-display').textContent = `Para ${monthDate.toLocaleString('es-ES', { month: 'long', year: 'numeric', timeZone: 'UTC' })}`;
                    document.getElementById('strategic-theme').textContent = summary.theme;
                    document.getElementById('business-goal').textContent = summary.goal;

                    populateFilters(monthData);
                    initChart(monthData);
                    applyFilters();
                }
            }

            function populateMonthSelector() {
                const monthKeys = [...new Set(allContentData.map(item => {
                    if (!item.publishDate) return null;
                    return `${item.publishDate.getUTCFullYear()}-${item.publishDate.getUTCMonth()}`;
                }))].filter(Boolean);

                monthKeys.sort((a, b) => new Date(a.split('-')[0], a.split('-')[1]) - new Date(b.split('-')[0], b.split('-')[1]));

                monthSelector.innerHTML = '';
                monthKeys.forEach(key => {
                    const [year, month] = key.split('-');
                    const date = new Date(Date.UTC(year, month));
                    const option = document.createElement('option');
                    option.value = key;
                    option.textContent = date.toLocaleString('es-ES', { month: 'long', year: 'numeric', timeZone: 'UTC' });
                    monthSelector.appendChild(option);
                });
                
                if (monthKeys.length > 0) {
                    monthSelector.value = monthKeys[monthKeys.length -1];
                }
            }

            function postDataLoad() {
                if (allContentData.length === 0) {
                    document.getElementById('app').innerHTML = `<div class="bg-yellow-100 border border-yellow-400 text-yellow-700 px-4 py-3 rounded-xl relative" role="alert"><strong class="font-bold">¬°Sin datos!</strong> <span class="block sm:inline">No se encontraron datos de contenido.</span></div>`;
                    return;
                }
                populateMonthSelector();
                renderForMonth(monthSelector.value);

                monthSelector.addEventListener('change', () => renderForMonth(monthSelector.value));
                filterType.addEventListener('change', applyFilters);
                filterFormat.addEventListener('change', applyFilters);
                filterStatus.addEventListener('change', applyFilters);
                modalCloseBtn.addEventListener('click', closeModal);
                modal.addEventListener('click', (e) => { if (e.target === modal) closeModal(); });
                document.addEventListener('keydown', (e) => { if (e.key === "Escape") closeModal(); });
            }
            
            function loadData() {
                // ===============================================================================================
                // ACTUALIZACI√ìN MENSUAL: PEGUE SUS DATOS AQU√ç
                // Copie los datos de su hoja de c√°lculo y p√©guelos dentro de los corchetes [] y llaves {} a continuaci√≥n.
                // ===============================================================================================

                allContentData = [
                    // DATOS DE SEPTIEMBRE DE 2025
