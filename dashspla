                    { publishDate: new Date("2025-09-09T00:00:00Z"), title: "[Insights] Generaciones y h√°bitos de consumo", link: "https://docs.google.com/document/d/1Bjgeu5Lz-Ww5x0ZuAZSzber7pasEpDAJDeuLxWo0E5U/edit?tab=t.0", status: "Confirmado", channels: ["LinkedIn"], format: "Infogr√°fico", type: "Original", synopsis: "No centres tu marketing en la Gen Z. Los Millennials son clave. Habla a todas las generaciones con t√°cticas diversas para un plan estrat√©gico integral.", starter: "¬øMarketing solo para Gen Z? ¬°Error! Los #Millennials lideran el consumo. No olvides a otras generaciones. ¬°Descubre 4 t√°cticas clave! #Generaciones", },
                    { publishDate: new Date("2025-09-12T00:00:00Z"), title: "Think Event: C√≥mo L‚ÄôOr√©al Chile transform√≥ su estrategia de b√∫squeda con IA", link: "https://docs.google.com/document/d/1FK36j5sk9MvSlku__lZVg3pFch3gkidYN9ly1mZBBJw/edit?tab=t.0", status: "Confirmado", channels: ["Sitio"], format: "Art√≠culo", type: "Original", synopsis: "L'Or√©al Chile duplic√≥ su tasa de conversi√≥n y redujo costos con IA Max en campa√±as de B√∫squeda, adapt√°ndose a la nueva era de b√∫squeda conversacional y visual.", starter: "Descubre c√≥mo L'Or√©al Chile duplic√≥ conversiones y redujo costos con IA Max para B√∫squeda. ¬°Aprende de su √©xito y transforma tu estrategia!", },
                    { publishDate: new Date("2025-09-18T00:00:00Z"), title: "Think Event: C√≥mo BBVA multiplic√≥ por 2,5 su asociaci√≥n con viajes gracias a YouTube", link: "https://docs.google.com/document/d/1SVTbhB386qsqV1UFAVl5IwKP2UgX1VAbR-EjRwYoitY/edit?tab=t.0", status: "Confirmado", channels: ["Sitio"], format: "Art√≠culo", type: "Original", synopsis: "BBVA recuper√≥ su posicionamiento estrat√©gico y multiplic√≥ su asociaci√≥n con viajes 2.5x usando YouTube y una estrategia YouTube first en TV Conectada.", starter: "Descubre c√≥mo BBVA recuper√≥ su posici√≥n clave con YouTube. ¬°Multiplic√≥ por 2.5 su asociaci√≥n con viajes! Lee m√°s: [link]", },
                    { publishDate: new Date("2025-09-19T00:00:00Z"), title: "¬°Descubre el poder de Am√©rica Latina en los 20 a√±os de YouTube! (video Karla)", link: "https://docs.google.com/document/d/1CKdzYxCjQp6_yBYNtZXCyjfLvE7YZrVYuWfu_keuKj8/edit?tab=t.s7i6kiuvvklq", status: "Confirmado", channels: ["LinkedIn"], format: "V√≠deo", type: "Original", synopsis: "YouTube celebra 20 a√±os con un reporte de Cultura y Tendencias, analizando su impacto en comunidades globales y el entretenimiento en Am√©rica Latina.", starter: "¬°Feliz 20 a√±os, YouTube! Descubre el reporte de Cultura y Tendencias 2025 y c√≥mo YouTube ha cambiado el entretenimiento. ¬°L√©elo ya! ‚û°Ô∏è [Link]", },
                    { publishDate: new Date("2025-09-24T00:00:00Z"), title: "Pasos simples para usar IA en tu marketing diario", link: "TBP", status: "Confirmado", channels: ["Sitio"], format: "Art√≠culo", type: "Tradu√ß√£o", synopsis: "TBP.", starter: "TBP", },
                    { publishDate: new Date("2025-09-25T00:00:00Z"), title: "Think Event: Case Mercado Libre", link: "TBP", status: "Confirmado", channels: ["Sitio"], format: "Art√≠culo", type: "Original", synopsis: "TBP.", starter: "TBP.", },
