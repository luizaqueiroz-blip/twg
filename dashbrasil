    <div id="app" class="container mx-auto p-4 sm:p-6 md:p-8">

        <header class="mb-10 pb-6 border-b border-slate-200 md:flex md:items-center md:justify-between">
            <div>
                <h1 class="text-3xl md:text-4xl font-bold text-slate-900">Think with Google Brasil: Radar de Conte√∫do</h1>
                <p id="month-display" class="text-lg text-slate-600 mt-2"></p>
            </div>
            <div class="mt-4 md:mt-0">
                <label for="month-selector" class="sr-only">Selecione o M√™s</label>
                <select id="month-selector" class="w-full md:w-auto p-2.5 border border-slate-300 rounded-lg shadow-sm focus:ring-2 focus:ring-blue-500 focus:border-blue-500 font-semibold bg-white">
                </select>
            </div>
        </header>

        <section class="mb-10 grid grid-cols-1 md:grid-cols-2 gap-6">
            <div class="p-6 bg-white rounded-xl shadow-sm border border-slate-200">
                <h2 class="text-xl font-bold text-slate-900 mb-4">Artigo em Destaque do M√™s</h2>
                <div class="space-y-4">
                    <div class="p-5 bg-slate-50 rounded-lg">
                        <h3 id="strategic-theme" class="text-lg font-semibold text-blue-600"></h3>
                    </div>
                    <div class="p-5 bg-slate-50 rounded-lg">
                        <h3 class="text-sm font-semibold text-slate-500 uppercase tracking-wider mb-2">üéØ Objetivo de Neg√≥cio</h3>
                        <p id="business-goal" class="text-base font-medium text-slate-700"></p>
                    </div>
                </div>
            </div>
            <div class="p-6 bg-white rounded-xl shadow-sm border border-slate-200 flex flex-col">
                <h2 class="text-xl font-bold text-slate-900 mb-4">M√©tricas Principais da Plataforma</h2>
                <div class="grid grid-cols-2 gap-4 flex-grow">
                    <div class="p-5 bg-slate-50 rounded-lg flex flex-col items-center justify-center text-center">
