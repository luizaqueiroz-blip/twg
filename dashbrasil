        <main class="grid grid-cols-1 lg:grid-cols-12 gap-8">
            <aside class="lg:col-span-3">
                <div class="sticky top-8">
                    <div class="p-6 bg-white rounded-xl shadow-sm border border-slate-200 mb-6">
                        <h3 class="text-lg font-bold text-slate-900 mb-4">Mix de Conte√∫do</h3>
                        <div class="chart-container">
                            <canvas id="contentMixChart"></canvas>
                        </div>
                    </div>
                </div>
            </aside>

            <div class="lg:col-span-9">
                <div class="p-4 bg-white rounded-xl shadow-sm border border-slate-200 mb-6 flex flex-wrap items-center gap-4">
                    <h3 class="text-lg font-bold text-slate-900 mr-4">Filtros</h3>
                    <div class="flex-grow grid grid-cols-1 sm:grid-cols-3 gap-4">
                        <div>
                            <label for="filter-type" class="sr-only">Tipo</label>
                            <select id="filter-type" class="w-full p-2 border border-slate-300 rounded-lg shadow-sm focus:ring-2 focus:ring-blue-500 focus:border-blue-500">
                                <option value="all">Todos os Tipos</option>
                                <option value="Original">Original</option>
                                <option value="Tradu√ß√£o">Tradu√ß√£o</option>
                            </select>
                        </div>
                        <div>
                            <label for="filter-format" class="sr-only">Formato</label>
                            <select id="filter-format" class="w-full p-2 border border-slate-300 rounded-lg shadow-sm focus:ring-2 focus:ring-blue-500 focus:border-blue-500">
                                <option value="all">Todos os Formatos</option>
                            </select>
                        </div>
                         <div>
                            <label for="filter-status" class="sr-only">Status</label>
                            <select id="filter-status" class="w-full p-2 border border-slate-300 rounded-lg shadow-sm focus:ring-2 focus:ring-blue-500 focus:border-blue-500">
                                <option value="all">Todos os Status</option>
